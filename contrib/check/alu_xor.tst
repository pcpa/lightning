#include "alu.inc"

.code	$(4 * 1024 * 1024)
	prolog 0

#define XOR(N, T, I0, I1, V)	ALU(N, T, xor, I0, I1, V)
	XOR(0, i,	0x7fffffff, 1,		0x7ffffffe)
	XOR(1, i,	1,	    0x7fffffff, 0x7ffffffe)
	XOR(2, i,	0x80000000, 1,		0x80000001)
	XOR(3, i,	1,	    0x80000000, 0x80000001)
	XOR(4, i,	0x7fffffff, 0x80000000, 0xffffffff)
	XOR(5, i,	0x80000000, 0x7fffffff, 0xffffffff)
	XOR(6, i,	0x7fffffff, 0xffffffff, 0x80000000)
	XOR(7, i,	0xffffffff, 0x7fffffff, 0x80000000)
	XOR(9, i,	0xffffffff, 0xffffffff, 0)
	XOR(10, i,	0x7fffffff, 0,		0x7fffffff)
	XOR(11, i,	0,	    0x7fffffff,	0x7fffffff)
	XOR(0, ui,	0x7fffffff, 1,		0x7ffffffe)
	XOR(1, ui,	1,	    0x7fffffff, 0x7ffffffe)
	XOR(2, ui,	0x80000000, 1,		0x80000001)
