	XOR(10, ui,	0x7fffffff, 0,		0x7fffffff)
	XOR(11, ui,	0,	    0x7fffffff,	0x7fffffff)
#if __WORDSIZE == 32
	XOR(0, l,	0x7fffffff, 1,		0x7ffffffe)
	XOR(1, l,	1,	    0x7fffffff, 0x7ffffffe)
	XOR(2, l,	0x80000000, 1,		0x80000001)
	XOR(3, l,	1,	    0x80000000, 0x80000001)
	XOR(4, l,	0x7fffffff, 0x80000000, 0xffffffff)
	XOR(5, l,	0x80000000, 0x7fffffff, 0xffffffff)
	XOR(6, l,	0x7fffffff, 0xffffffff, 0x80000000)
	XOR(7, l,	0xffffffff, 0x7fffffff, 0x80000000)
	XOR(9, l,	0xffffffff, 0xffffffff, 0)
	XOR(10, l,	0x7fffffff, 0,		0x7fffffff)
	XOR(11, l,	0,	    0x7fffffff,	0x7fffffff)
	XOR(0, ul,	0x7fffffff, 1,		0x7ffffffe)
	XOR(1, ul,	1,	    0x7fffffff, 0x7ffffffe)
	XOR(2, ul,	0x80000000, 1,		0x80000001)
	XOR(3, ul,	1,	    0x80000000, 0x80000001)
	XOR(4, ul,	0x7fffffff, 0x80000000, 0xffffffff)
	XOR(5, ul,	0x80000000, 0x7fffffff, 0xffffffff)
	XOR(6, ul,	0x7fffffff, 0xffffffff, 0x80000000)
	XOR(7, ul,	0xffffffff, 0x7fffffff, 0x80000000)
	XOR(9, ul,	0xffffffff, 0xffffffff, 0)
	XOR(10, ul,	0x7fffffff, 0,		0x7fffffff)
	XOR(11, ul,	0,	    0x7fffffff,	0x7fffffff)
#else
	XOR(0, l,	0x7fffffff,	    1,			0x7ffffffe)
	XOR(1, l,	1,		    0x7fffffff,		0x7ffffffe)
	XOR(2, l,	0x80000000,	    1,			0x80000001)
	XOR(3, l,	1,		    0x80000000,		0x80000001)
	XOR(4, l,	0x7fffffff,	    0x80000000,		0xffffffff)
	XOR(5, l,	0x80000000,	    0x7fffffff,		0xffffffff)
	XOR(6, l,	0x7fffffff,	    0xffffffff,		0x80000000)
	XOR(7, l,	0xffffffff,	    0x7fffffff,		0x80000000)
	XOR(9, l,	0xffffffff,	    0xffffffff,		0)
	XOR(10, l,	0x7fffffff,	    0,			0x7fffffff)
	XOR(11, l,	0,		    0x7fffffff,		0x7fffffff)
	XOR(12, l,	0x7fffffffffffffff, 1,			0x7ffffffffffffffe)
