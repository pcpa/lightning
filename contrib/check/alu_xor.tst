#undef XOR
#define XOR(N, T, I0, V)	ALUC(N, T, xor, I0, V)
	XOR(0, i,	0,			0)
	XOR(1, i,	0x7fffffff,		0)
	XOR(2, i,	0x80000000,		0)
	XOR(3, i,	0xffffffff,		0)
	XOR(0, ui,	0,			0)
	XOR(1, ui,	0x7fffffff,		0)
	XOR(2, ui,	0x80000000,		0)
	XOR(3, ui,	0xffffffff,		0)
#if __WORDSIZE == 32
	XOR(0, l,	0,			0)
	XOR(1, l,	0x7fffffff,		0)
	XOR(2, l,	0x80000000,		0)
	XOR(3, l,	0xffffffff,		0)
	XOR(0, ul,	0,			0)
	XOR(1, ul,	0x7fffffff,		0)
	XOR(2, ul,	0x80000000,		0)
	XOR(3, ul,	0xffffffff,		0)
#else
	XOR(0, l,	0,			0)
	XOR(1, l,	0x7fffffff,		0)
	XOR(2, l,	0x80000000,		0)
	XOR(3, l,	0xffffffff,		0)
	XOR(4, l,	0x7fffffffffffffff,	0)
	XOR(5, l,	0x8000000000000000,	0)
	XOR(6, l,	0xffffffffffffffff,	0)
	XOR(0, ul,	0,			0)
	XOR(1, ul,	0x7fffffff,		0)
	XOR(2, ul,	0x80000000,		0)
	XOR(3, ul,	0xffffffff,		0)
	XOR(4, ul,	0x7fffffffffffffff,	0)
	XOR(5, ul,	0x8000000000000000,	0)
	XOR(6, ul,	0xffffffffffffffff,	0)
#endif

	ret
